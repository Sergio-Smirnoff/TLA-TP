"+" -> PLUS;
"-" -> MINUS;
"*" -> TIMES;
"/" -> SLASH;
"("  -> LPAREN;
")" -> RPAREN;
"="  -> EQL;
"<>" -> NEQ;
"<"  -> LSS;
">"  -> GTR;
"<=" -> LEQ;
">=" -> GEQ;

Regex digit         [0-9];

{digit}+ -> (){
                       var.attribute.num = Integer.parseInt(var.text.toString());
                       return NUMBER;
};

/*Skipps white spaces*/ 
"\t";
"\n";
"\r";

"default" -> (){ return REJECT; };
